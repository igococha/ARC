        <logger logEvery="100" fileName="examples/testARCcatfull.$(seed).log">
            <model idref='posterior' />
            <log idref="posterior" />
            <log idref="treeLikelihood" />
            <log idref="prior" />
            <log idref="coalescent" />
            <log idref="popSize" />
            <log spec='beast.evolution.tree.TreeHeightLogger' tree='@tree' />
            <log idref="rateOmega" />
            <log id='rate' spec='RateStatistic' tree='@tree' branchratemodel='@branchRates'/>
            <log idref="hky.kappa" />
            <log idref="rateCategories" /> 
        </logger>
  -->      
        <!-- <logger logEvery="100" fileName="test.$(seed).trees">
            <log idref="tree" />
        </logger> -->
        <logger logEvery="10">
            <log idref="rateOmega" />
            <model idref='posterior' />
            <log idref="posterior" />
            <log idref="treeLikelihood" />
            <log idref="coalescent" />
            <log idref="popSize" />
            <log spec='beast.evolution.tree.TreeHeightLogger' tree='@tree' />
           <log id='screenrate' spec='RateStatistic' tree='@tree' branchratemodel='@branchRates'/>             
            <log idref="hky.kappa" />
            <log idref="rateProbs" /> 
        </logger>
    </run>

</beast>
